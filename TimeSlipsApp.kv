<TimeSlipsApp>
	source: root.source
	headline:root.headline
	caption:root.caption
	pos_hint:{"top":1}
	size_hint:(1,1)
	img: asyn_image
	AsyncImage:
		id:asyn_image
		nocache: True
		allow_stretch:True
		keep_ratio:True
		width: self.parent.width
		height:self.parent.width/self.image_ratio			
		size_hint:(1,1)
